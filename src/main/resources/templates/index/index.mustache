{{>layout/html-header}}
<link rel="stylesheet" href="/css/index.css"> 
<script src="/js/index.js"></script>

<main role="main">

<section id="search-wrp" class="container">
	<form role="form" action="search" method="post" id="id-search-form" autocomplete="off">
		<div class="form-group">
			<input type="text" name="id" class="form-control" >
		</div>			 	
	 	<div class="form-group">
	 		<button type="submit" id="btn-submit-id-search" class="btn btn-primary btn-block blue lighten-1 waves-effect waves-light">Search</button>
 		</div>
	</form>
	
	<form role="form" id="map-search-form">
		<div class="form-row">
			<div class="form-group col-sm">
				<input type="text" class="form-control">
			</div>
			<div class="form-group col-sm">
				<input type="text" class="form-control">
			</div>
			<div class="form-group col-sm">
				<input type="text" class="form-control">
			</div>
		</div>		
		<div class="form-row">
			<div class="form-group col-sm-4">
		 		<button type="button" class="btn-change-search btn btn-outline-primary btn-block">ID</button>
		 	</div>
		 	<div class="form-group col-sm-8">
		 		<button type="submit" id="btn-submit-map-search" class="btn btn-primary btn-block">Search</button>
	 		</div>
		</div>
 	</form>
</section>	
	

<section id="result-wrp" class="container text-center">
	<div class="container">		
		<table id="result-id-table" class="table centered">
			<thead>
		        <tr>
		            <th>Owner Name</th>
		            <th>Bussiness Name</th>
		            <th>Status</th>
		        </tr>
		    </thead>
		</table>
		
	</div>
</section>

{{>index/index-modal}}
</main>
{{>layout/html-footer}}
